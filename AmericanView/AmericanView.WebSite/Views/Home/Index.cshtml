    @section Scripts {
        <script src="@Url.Content("/Content/plugins/jquery-ui/jquery-ui.min.js")" type="text/javascript"></script>

        <script type="text/javascript">
            $(document).ready(function () {
                $("#owl").owlCarousel({
                    autoPlay: 3000,
                    navigation: false,
                    slideSpeed: 300,
                    paginationSpeed: 400,
                    singleItem: true
                });

                $("#header").sticky({ topSpacing: 0 });
            });
        </script>
    }

